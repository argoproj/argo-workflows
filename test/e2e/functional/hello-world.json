{
  "apiVersion": "argoproj.io/v1alpha1",
  "kind": "Workflow",
  "metadata": {
    "generateName": "hello-world-"
  },
  "spec": {
    "entrypoint": "whalesay",
    "templates": [
      {
        "name": "whalesay",
        "container": {
          "image": "argoproj/argosay:v1",
          "command": [
            "cowsay"
          ],
          "args": [
            "hello world"
          ]
        }
      }
    ]
  }
}
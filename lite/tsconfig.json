{
    "compilerOptions": {
      "target": "es6",
      "module": "commonjs",
      "moduleResolution": "node",
      "sourceMap": true,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "removeComments": false,
      "noImplicitAny": false,
      "types": [
          "request",
          "express",
          "node"
      ],
      "outDir": "dist"
    },
    "exclude": [
      "node_modules",
      "dist"
    ]
  }
